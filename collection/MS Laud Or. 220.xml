                    <physDesc>
                        <p>ff. 328.Compilation of various MSS, mostly written in the same type of
                            script and sharing the same watermarks.</p>

                        <objectDesc form="codex">

                            <supportDesc material="chart">

                                <support> Paper <watermark/>
                                </support>
                                <extent> ff.328 <dimensions type="leaf" unit="mm">
                                        <height quantity="198"/>
                                        <width quantity="150"/>
                                    </dimensions>
                                </extent>
                                <foliation>Foliation in Arabic numerals, fol. 210 is double, fols.
                                    307-308 are bound in wrong order.</foliation>
                                <collation> x quires + 2 leaves inner/outer leaf of quire
                                        <signatures>Hebrew/Arabic characters</signatures>
                                    <catchwords>catchwords, repeated words, marked central
                                        opening</catchwords>
                                </collation>
                                <condition/>
                            </supportDesc>
                            <layoutDesc>
                                <layout columns="1"> Text in one column, except fols. 316r-318v:
                                    text in two columns. </layout>
                            </layoutDesc>
                        </objectDesc>
                        <handDesc>
                            <summary>Sephardi cursive and semi-cursive scripts; fol. 232r-237r:
                                Byzantine script</summary>
                            <handNote scope="major" script="sephardi" rend="semi-cursive"
                            > </handNote>
                        </handDesc>
                        <decoDesc>
                            <decoNote type="unspecified"> Not decorated. </decoNote>
                        </decoDesc>
                        
                        <additions/>
                        <bindingDesc><p>Light brown leather binging.</p></bindingDesc>

                    </physDesc>
                    <history>
