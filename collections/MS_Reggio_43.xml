               <msPart xml:id="MS_Reggio_43-part1" n="1">
                  <msIdentifier>
                     <altIdentifier type="partial">
                        <idno>MS. Reggio 43/1</idno>
                     </altIdentifier>
                  </msIdentifier>
                  <msContents>
                     <summary>Collection of astrological and astronomical texts</summary>
                     <msItem xml:id="MS_Reggio_43-part1-item1" n="1">
                        <locus>ff. 1r-41v</locus>
                        <author ref="http://viaf.org/viaf/13522985" key="person_680">Duran, Profiat, approximately 1350-approximately 1415דורן, שמעון בן צמח, 1361-1444</author>
                        <title>Heshev ha-ʾefod</title>
                        <title xml:lang="he">חשב האפד</title>
                        <textLang mainLang="he">Hebrew</textLang>
                     </msItem>
                  </msContents>
                  <physDesc>
                     <objectDesc form="codex">
                        <supportDesc material="chart">
                           <support>Paper with outer and inner sheets of quires made of parchment <watermark>(watermarks similar to Briquet no. 13932, dated 1393 and 1397 [also 1389-1420])</watermark>
                           </support>
                           <extent>ff. 41 <dimensions type="leaf" unit="mm">
                                 <height quantity="215"/>
                                 <width quantity="135"/>
                              </dimensions>
                           </extent>
                           <foliation>Foliation in Arabic numerals.</foliation>
                           <collation>
                              <signatures>no signatures</signatures>
                              <catchwords>catchwords on almost every verso</catchwords>
                           </collation>
                        </supportDesc>
                        <layoutDesc>
                           <layout columns="1">Text in one column.</layout>
                        </layoutDesc>
                     </objectDesc>
                     <handDesc>
                        <summary>Sephardi semi-cursive and cursive script.</summary>
                        <handNote scope="sole" script="sephardi" rend="semi-cursive">Semi cursive script fols. 1r-37v</handNote>
                        <handNote scope="sole" script="sephardi" rend="cursive">cursive script fols. 37v-41v</handNote>
                        <handNote scope="sole" script="sephardi" rend="semi-cursive">Fol. 25 by another hand.</handNote>
                     </handDesc>
                     <decoDesc>
                        <decoNote type="diagram">Diagrams inserted into the body of the text (fols. 8r, 10v)</decoNote>
                     </decoDesc>
                     <additions>
                        <ref target="#MS_Reggio_43_2_a">End is completed by later hand.</ref>
                     </additions>
                  </physDesc>
                  <history>
                     <origin>
                        <origPlace>[Provence?]</origPlace>
                        <origDate when="1400">[ca. 1400]</origDate>
                     </origin>
                  </history>
               </msPart>
